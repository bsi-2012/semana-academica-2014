<div class="wrapper">
  <h2 class="section-title"><%= @labels['sponsors'] %></h2>
</div>

  <div class="sponsors-category sponsors-category-partner">
    <div class="wrapper">
      <!-- <h3 class="sponsors-category-title">Patroc√≠nio</h3> -->
      <ul class="sponsors-adamantium">
      <% for sponsorAdamantium in @sponsors.adamantium: %>
        <li class="sponsor-item <%= sponsorAdamantium.logoClass %>" itemscope itemtype="http://schema.org/Organization">
          <a href="<%= sponsorAdamantium.url %>" class="sponsor-logo" itemprop="url">
            <img src="<%= sponsorAdamantium.logo %>" alt="<%= sponsorAdamantium.name %>" class="photo <%= sponsorAdamantium.logoClassImg %>" itemprop="image">
          </a>
        </li>
      <% end %>
      </ul>
    </div>
  </div>